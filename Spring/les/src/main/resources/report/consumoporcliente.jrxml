	<queryString language="SQL">
		<![CDATA[SELECT cli.id,
	cli.nome,
	SUM( CP.preco) AS "valor_total"
FROM cliente cli
	RIGHT JOIN compra com ON 
	 com.cliente_id = cli.id 
	LEFT JOIN compra_produto CP ON 
	 CP.compra_id = com.id 
GROUP BY cli.id]]>
	</queryString>
